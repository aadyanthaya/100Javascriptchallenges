//Write a function that takes an array of objects and a key and returns a new array with only the objects that have a falsy value for that key.

function objectArrayFalsy(arrayaya, key) {
  const filteredA = arrayaya.filter((obj) => !obj[key]);
  return filteredA;
}
const inputArrayaFalse = [
  { name: "Alice", age: 25, active: true },
  { name: "Bob", age: 30, active: false },
  { name: "Charlie", age: 20, active: true },
  { name: "David", age: 35, active: true }
];

const filteredArrya = objectArrayFalsy(inputArrayaFalse, "active");
console.log(filteredArrya);

//Write a function that takes a number and returns true if it is a palindrome, false otherwise.
function palindromeNum(Num) {
  let n = Num.length;
  for (let i = 0, j = n - 1; i < n && 0 < j; i++, j--) {
    if (Num[i] !== Num[j]) {
      console.log(n);
      return false;
    }
  }

  return true;
}
console.log(palindromeNum("12321"));
